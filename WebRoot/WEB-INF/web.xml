        <servlet-class>edu.ahut.web.controller.SubmitThesisServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowThesisInfoServlet</servlet-name>
        <servlet-class>edu.ahut.web.controller.ShowThesisInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/servlet/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginUIServlet</servlet-name>
        <url-pattern>/servlet/LoginUIServlet</url-pattern>
    </servlet-mapping>
