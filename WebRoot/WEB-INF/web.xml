        <servlet-class>edu.ahut.web.UI.RegisterUIServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>edu.ahut.web.controller.RegisterServlet</servlet-class>
    </servlet>
